<p class="p1">&lt;script&gt;</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; </span>function showTip(textarea) {</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; &nbsp; </span>const tip = textarea.nextElementSibling;</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; &nbsp; </span>if (textarea.value.trim() !== "") {</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; &nbsp; &nbsp; </span>tip.style.display = "block";</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; &nbsp; </span>} else {</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; &nbsp; &nbsp; </span>tip.style.display = "none";</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; &nbsp; </span>}</p>
<p class="p1"><span class="Apple-converted-space">&nbsp; </span>}</p>
<p class="p1">&lt;/script&gt;</p>
